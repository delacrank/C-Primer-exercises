#include <iostream>
#include <stdexcept>

using namespace std;
    
int main() {
    float first = 0, second = 0;
    
    begin :
        cin >> first >> second;
    
    try{
        if(second == 0)
            throw runtime_error("the second number must not be zero.");
        cout << first/second << endl; 
    } catch (runtime_error err)
    {
        cout << err.what() << endl;
        cout << "Enter a two new numbers" << endl;
        goto begin;
    }
}
    

    
