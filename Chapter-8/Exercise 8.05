    #include <fstream>
    #include <string>
    #include <iostream>
    #include <vector>

    int main()
    {
        std::vector<std::string> vec;
        std::ifstream file("ok.txt");
        std::string word;
        while(file >> word)
            vec.push_back(word);
        
        for(int i = 0; i < vec.size(); i++)
            std::cout << vec[i] << std::endl;
    }
    
